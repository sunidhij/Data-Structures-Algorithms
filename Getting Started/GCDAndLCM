#include <iostream>
using namespace std;

int main(int argc, char **argv)
{
    
      int n1,n2;
      cin>>n1>>n2;
      
      //GCD
      int m = n1;
      int n = n2;
      while (m!= n)
      { 
          if ( m>n)
          {
              m = m-n;
          }
          else {
              n = n-m;
          }
      }
     cout<<m<<endl;
      //LCM
      // gcd x lcm = n1 x  n2
      int lcm = (n1*n2)/m;
       cout<<lcm<<endl;
      
    
}
